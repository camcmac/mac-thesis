@article{de_haan_floating_2019,
	title = {Floating microplastics and aggregate formation in the Western Mediterranean Sea},
	volume = {140},
	issn = {0025326X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0025326X19300748},
	doi = {10.1016/j.marpolbul.2019.01.053},
	abstract = {Pollution by large-sized plastics and microplastic debris is widespread in all Earth environments, also threatening marine ecosystems worldwide. In this study we determine the load of microplastics in the Western Mediterranean Sea and evaluate their aggregation potential into marine aggregates. We report average microplastic abundances of 0.10 ± 0.09 items m−2 (max: 0.50 items m−2). Abundances and chemical composition of microplastics are subject to temporal changes as a function of human activities in the areas of inﬂuence and shifting meteorological and sea-state conditions. We ﬁnd that microplastics were on average smaller in mass than other studies conducted in the Mediterranean Sea indicating longer exposure times in the environment. The microplastic aggregation potential was determined by inspecting formed biogenic aggregates either during sample collection or in the laboratory. Smaller and more angular microplastics dominated in marine aggregates, representing an average 40\% in abundance and 25\% in mass of microplastics.},
	pages = {523--535},
	journaltitle = {Marine Pollution Bulletin},
