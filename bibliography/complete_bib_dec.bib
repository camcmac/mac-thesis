	langid = {english},
	file = {Yang et al. - 2019 - Plastics in the marine environment are reservoirs .pdf:C\:\\Users\\Lal\\Zotero\\storage\\VD4YC5FP\\Yang et al. - 2019 - Plastics in the marine environment are reservoirs .pdf:application/pdf},
}

