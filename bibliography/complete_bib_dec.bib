	journaltitle = {Environmental Pollution},
	shortjournal = {Environmental Pollution},
	author = {Matiddi, Marco and Hochsheid, Sandra and Camedda, Andrea and Baini, Matteo and Cocumelli, Cristiano and Serena, Fabrizio and Tomassetti, Paolo and Travaglini, Andrea and Marra, Stefano and Campani, Tommaso and Scholl, Francesco and Mancusi, Cecilia and Amato, Ezio and Briguglio, Paolo and Maffucci, Fulvio and Fossi, Maria Cristina and Bentivegna, Flegra and de Lucia, Giuseppe Andrea},
