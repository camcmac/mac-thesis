@article{von_dassow_biological_2014,
	title = {Biological oceanography, biogeochemical cycles, and pelagic ecosystem functioning of the east central South Pacific Gyre: focus on Easter Island and Salas y Gomez Island},
	volume = {42},
	issn = {0718560X, 0718560X},
	url = {http://www.lajar.cl/pdf/imar/v42n4/Articulo_42_4_4.pdf},
	doi = {10.3856/vol42-issue4-fulltext-4},
	shorttitle = {Biological oceanography, biogeochemical cycles, and pelagic ecosystem functioning of the east central South Pacific Gyre},
	abstract = {The Exclusive Economic Zone of Chile defined by Easter Island and Salas y Gómez Island is in the South Pacific Sub-tropical Gyre ({SPSG}), putting it at the center of the most oligotrophic and biomass poor waters in the world. Only 10 biological oceanographic expeditions have entered this zone in 105 years (19052010). We review key aspects of the plankton ecosystem and biogeochemical function relevant for the understanding of and conservation planning for marine environments. Plankton production is limited by lack of dissolved inorganic fixed nitrogen, not phosphorous. Higher organic nitrogen levels might be biologically unavailable. Short-term experiments suggested iron is not limiting, yet iron still likely limits nitrogen fixation, and thus production, at longer time scales, as the presence of nitrogen-fixers is exceptionally low compared to other ocean gyres. Plankton function is dominated by the smallest unicellular organisms, picoplankton ({\textless}3 µm in diameter). The {SPSG} represents a center of high biodiversity for picoplankton, as well as heterotrophic organisms such as tinntinids, siphonophores, and possibly amphipods, although data for key zooplankton, such as copepods, are lacking. Many groups exhibit negative relationships between diversity and total plankton biomass. High diversity might result from dispersal from a very large metacommunity and minimal competition within functional groups. Whether an island-mass effect causes a real or apparent increase in plankton biomass around Easter Island must be confirmed by high-resolution sampling in situ. Long-term threats to the planktonic ecosystem may include climate change-enhanced ocean stratification and plastic marine debris accumulation. Finally, priorities for future research are highlighted.},
	pages = {703--742},
	number = {4},
	journaltitle = {Latin American Journal of Aquatic Research},
	shortjournal = {lajar},
	author = {von Dassow, Peter and Collado Fabbri, Silvana},
	urldate = {2021-12-24},
	date = {2014-10-10},
	langid = {english},
	file = {von Dassow and Collado Fabbri - 2014 - Biological oceanography, biogeochemical cycles, an.pdf:C\:\\Users\\Lal\\Zotero\\storage\\NUNYZWFF\\von Dassow and Collado Fabbri - 2014 - Biological oceanography, biogeochemical cycles, an.pdf:application/pdf},
}

