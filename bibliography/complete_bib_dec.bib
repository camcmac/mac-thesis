	shortjournal = {R. Soc. open sci.},
	author = {Woodall, Lucy C. and Sanchez-Vidal, Anna and Canals, Miquel and Paterson, Gordon L.J. and Coppock, Rachel and Sleight, Victoria and Calafat, Antonio and Rogers, Alex D. and Narayanaswamy, Bhavani E. and Thompson, Richard C.},
	urldate = {2021-12-24},
	date = {2014-12},
	langid = {english},
	file = {Woodall et al. - 2014 - The deep sea is a major sink for microplastic debr.pdf:C\:\\Users\\Lal\\Zotero\\storage\\N2NE4V77\\Woodall et al. - 2014 - The deep sea is a major sink for microplastic debr.pdf:application/pdf},
}

@article{woodall_deep-sea_2015,
