	file = {Consoli et al. - 2019 - Marine litter from fishery activities in the Weste.pdf:C\:\\Users\\Lal\\Zotero\\storage\\EFGZH9GX\\Consoli et al. - 2019 - Marine litter from fishery activities in the Weste.pdf:application/pdf},
}

@article{constantino_abundance_2019,
	title = {Abundance and composition of floating marine macro litter on the eastern sector of the Mediterranean Sea},
	volume = {138},
