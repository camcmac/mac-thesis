@article{hartmann_are_2019,
	title = {Are We Speaking the Same Language? Recommendations for a Definition and Categorization Framework for Plastic Debris},
	volume = {53},
	issn = {0013-936X, 1520-5851},
	url = {https://pubs.acs.org/doi/10.1021/acs.est.8b05297},
	doi = {10.1021/acs.est.8b05297},
	shorttitle = {Are We Speaking the Same Language?},
	abstract = {The accumulation of plastic litter in natural environments is a global issue. Concerns over potential negative impacts on the economy, wildlife, and human health provide strong incentives for improving the sustainable use of plastics. Despite the many voices raised on the issue, we lack a consensus on how to deﬁne and categorize plastic debris. This is evident for microplastics, where inconsistent size classes are used and where the materials to be included are under debate. While this is inherent in an emerging research ﬁeld, an ambiguous terminology results in confusion and miscommunication that may compromise progress in research and mitigation measures. Therefore, we need to be explicit on what exactly we consider plastic debris. Thus, we critically discuss the advantages and disadvantages of a uniﬁed terminology, propose a deﬁnition and categorization framework, and highlight areas of uncertainty. Going beyond size classes, our framework includes physicochemical properties (polymer composition, solid state, solubility) as deﬁning criteria and size, shape, color, and origin as classiﬁers for categorization. Acknowledging the rapid evolution of our knowledge on plastic pollution, our framework will promote consensus building within the scientiﬁc and regulatory community based on a solid scientiﬁc foundation.},
	pages = {1039--1047},
	number = {3},
	journaltitle = {Environmental Science \& Technology},
	shortjournal = {Environ. Sci. Technol.},
	author = {Hartmann, Nanna B. and Hüffer, Thorsten and Thompson, Richard C. and Hassellöv, Martin and Verschoor, Anja and Daugaard, Anders E. and Rist, Sinja and Karlsson, Therese and Brennholt, Nicole and Cole, Matthew and Herrling, Maria P. and Hess, Maren C. and Ivleva, Natalia P. and Lusher, Amy L. and Wagner, Martin},
	urldate = {2021-12-27},
	date = {2019-02-05},
	langid = {english},
	file = {Hartmann et al. - 2019 - Are We Speaking the Same Language Recommendations.pdf:C\:\\Users\\Lal\\Zotero\\storage\\NBLANJ7E\\Hartmann et al. - 2019 - Are We Speaking the Same Language Recommendations.pdf:application/pdf},
